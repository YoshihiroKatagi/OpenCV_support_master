    "import cv2\r\n",
    "# コールバック用の関数\r\n",
    "def printing(x):\r\n",
    "    print(x)\r\n",
    "\r\n",
    "initial_value = 100\r\n",
    "cv2.namedWindow(\"img\")\r\n",
    "# トラックバーを作成する\r\n",
    "cv2.createTrackbar(\"track\",\"img\", initial_value, 255, printing)\r\n",
    "cv2.waitKey(0)\r\n",
    "cv2.destroyAllWindows()"
   ],
